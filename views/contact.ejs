<%- include('partials/header') -%>
<!-- FORM -->
<section class="form">
  <h1 class="page-title">Contact</h1>
  <form action="#" class="contact-form">
    <div class="form-row">
      <div class="col-sm-6">
        <input
          type="text"
          class="form-control contact-text"
          placeholder="Your Name"
        />
      </div>
      <div class="col-sm-6">
        <input
          type="text"
          class="form-control contact-text"
          placeholder="Your E-Mail"
        />
      </div>
      <div class="form-group col form-box">
        <textarea
          class="form-control contact-text"
          id="textarea"
          rows="3"
          placeholder="Your Message"
        ></textarea>
      </div>
    </div>
    <input
      type="submit"
      id="email-submit"
      class="email-btn contact-btn btn btn-light btn-lg"
      value="Send"
    />
  </form>
</section>
<%- include('partials/footer') -%>
